#! /bin/sh
if test -z "$PAPARAZZI_SRC"; then
    PAPARAZZI_SRC=/usr/share/paparazzi
    PAPARAZZI_BIN=$PAPARAZZI_SRC/bin
else
    PAPARAZZI_BIN=$PAPARAZZI_SRC/sw/tools
fi
ocaml -I $PAPARAZZI_SRC/sw/tools -I /usr/lib/ocaml/xml-light -I /usr/lib/ocaml/pcre -I /usr/lib/ocaml -I /usr/lib/ocaml -I /usr/lib/ocaml/netsys -I /usr/lib/ocaml/netstring -I /usr/lib/ocaml -I /usr/lib/ocaml -I /usr/lib/ocaml/netsys -I /usr/lib/ocaml/pcre -I /usr/lib/ocaml/netstring -I /usr/lib/ocaml/equeue -I /usr/lib/ocaml/netclient -I $PAPARAZZI_SRC/sw/lib/ocaml unix.cma str.cma ivy-ocaml.cma xml-light.cma pcre.cma unix.cma bigarray.cma netsys_oothr.cma netsys.cma netstring.cma netaccel.cma netaccel_link.cmo unix.cma bigarray.cma netsys_oothr.cma netsys.cma pcre.cma netstring.cma netaccel.cma netaccel_link.cmo equeue.cma netclient.cma lib-pprz.cma gen_common.cmo $PAPARAZZI_BIN/gen_periodic.ml $*
