<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <!-- I tried to use the app.config file to do a string prompt lookup, but was having trouble with permissions, so I generated
  a static dictionary in the HMI Class-->
  <configSections>
    <section name="UserPrompt.appSettings" type="System.Configuration.AppSettingsSection" />
  </configSections>
  <UserPrompts.appSettings>
    <add key="NewFile" value ="Configure New Output File? (Y/N)"></add>
  </UserPrompts.appSettings>
</configuration>